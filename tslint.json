{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "interface-over-type-literal": false,
    "member-access": [false],
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false,
    "quotemark": [true, "single"],
    "semicolon": [true, "never"]
  },
  "rulesDirectory": []
}
